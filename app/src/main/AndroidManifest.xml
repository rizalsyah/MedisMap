<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.tm.medismap">
<uses-permission android:name="android.permission.INTERNET"/>

<application
    android:allowBackup="true"
    android:icon="@mipmap/medismap"
    android:label="Medismap Webview"
    android:roundIcon="@mipmap/medismap"
    android:supportsRtl="true"
    android:screenOrientation="portrait"
    android:theme="@style/AppTheme.NoActionBar">
    <activity android:name=".Splashscreen">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name=".Masuk"
        android:theme="@style/AppTheme.NoActionBar"
        android:label="@string/app_name"
        android:configChanges="orientation"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".Daftar"
        android:theme="@style/AppTheme.NoActionBar"
        android:configChanges="orientation"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".MainActivity"
        android:theme="@style/AppTheme.NoActionBar"
        android:label="@string/app_name"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".Menu"
        android:theme="@style/AppTheme"
        android:label=""
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".ListDokter"
        android:theme="@style/AppTheme.NoActionBar"
        android:label="Dokter"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".ListFaskes"
        android:theme="@style/AppTheme"
        android:label="Fasilitas Kesehatan"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".DetailDokter"
        android:theme="@style/AppTheme.NoActionBar"
        android:screenOrientation="portrait">
    </activity>
    <activity
        android:name=".Booking"
        android:configChanges="orientation"
        android:screenOrientation="portrait">
    </activity>
</application>
</manifest>